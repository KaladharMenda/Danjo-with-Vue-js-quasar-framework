body {
	background: #d5dde1c4 !important;
	.saveBtn{
		margin-top:15px;
		padding:10px;
		width: 100%;
	}
	.layout-padding {
		@media $desktop {
			padding: 4rem 1rem;
		}

		@media $tab {
			padding: 3rem 1rem;
		}

		@media $mobile {
			padding: 2rem 0rem;
		}
	}

	.q-layout-page-container {
		padding-left: 250px!important;

		@media $mobile {
			padding-left: 0px!important;
		}

		@media $tab {
			padding-left: 0px!important;
		}
	}
	.q-card {
		padding: 15px;
		box-shadow: none;
		min-height: 100vh;

		.q-if-label {
			font-size: 12px;
		}

		.q-if-inverted.bg-primary {
			background: #1c374eb0 !important;
		}

		.q-card-container {
			padding-top: 0px !important;
			width: 280px;
		}
	}
	.fab-position {
		bottom: 30px;
		right: 60px;

		@media $tab {
			right: 35px;
		}

		@media $mobile {
			bottom: 15px;
			right: 15px;
		}
	}

	.q-table-container {
		box-shadow: none !important;
	}

	.dashboard-card {
		background-color: white;
		margin-top: 15px;
		border-radius: 		10px;
		box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
	}

	.card-item {
		max-width: 18rem;
		padding-left: 15px;
	}

	.icon {
		width:29%;
		padding-top:13%;
	}

	.avatar {
	    vertical-align: middle;
	    width: 60%;
	    height: 68%;
	    border-radius: 100%;
	    margin-left:50px;
	}
	.newspng{

    max-width:200px;
		max-height:200px;
	}
	/*.q-btn {
		background: #69c07a !important;
		color: white !important;
	}*/
	.mobile-avatar {
	    vertical-align: middle;
	    width: 54%;
	    height: 93%;
	    border-radius: 50%;
	}

	.qbt_close{
	  background: red !important;
	  color: white !important;
	}
	.bg-grey {
		background: #69c07a !important;
	}
	.q-tabs-bar {
	}
}
